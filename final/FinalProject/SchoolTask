public class SchoolTask : Task
{
    private string _signature;

    public SchoolTask(string name, string description, string signature) : base(name, description)
    {
        _signature = signature;
    }

    public SchoolTask(string name, string description, string notes, string evaluation, Accomplish accomplish, Periods period, string signature) : base(name, description, notes, evaluation, accomplish, period)
    {
        _signature = signature;
    }
}

